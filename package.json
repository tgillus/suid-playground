    "@typescript-eslint/parser": "6.16.0",
    "@vitest/coverage-v8": "1.1.0",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "release-it": "17.0.1",
    "testdouble": "3.20.1",
    "tsx": "4.7.0",
